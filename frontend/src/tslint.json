{
  "extends": "../tslint.json",
  "rules": {
    "adjacent-overload-signatures": false,
    "variable-name": false,
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
