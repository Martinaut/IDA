PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
SELECT ?element ?label ?description
WHERE {
	<###URI###> rdfs:label ?label .
    FILTER (lang(?label) = "###LANG###") .

    OPTIONAL {
        <###URI###> rdfs:comment ?description .
    	FILTER (lang(?description) = "###LANG###") .
    }

    VALUES(?element){
        (<###URI###>)
    }
}